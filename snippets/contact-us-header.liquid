  opacity: 0.85;
  padding-left:42px;
}

.btn {
  display: inline-block;
  margin-top: 10px;
  padding: 8px 15px;
  border-radius: 8px;
  font-weight: 600;
  box-shadow: 0 4px 4px rgba(0, 0, 0, 0.1);
  
}
.button-group {
  display: flex;
  flex-wrap: wrap;
  gap: 10px;
  width: 100%;
  text-align: center;
}

.button-row {
  display: flex;
  width: 100%;
  gap: 10px;
}

.button-row .btn {
  flex: 1;
  background-color: #ffde21;
  color: black;
  padding: 8px 12px;
